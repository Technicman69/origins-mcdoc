///     "type": "origins:selector_action",
///     "selector": "@e[type = minecraft:armor_stand, tag = some_tag]",
///     "bientity_action": {
///         "type": "origins:target_action",
///         "action": {
///             "type": "origins:execute_command",
///             "command": "kill @s"
///         }
///     }
/// }
