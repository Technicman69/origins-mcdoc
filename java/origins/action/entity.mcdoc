///     "item_condition": {
///         "type": "origins:armor_value",
///         "comparison": ">",
///         "compare_to": 0
///     },
///     "item_action": {
///         "type": "origins:damage",
///         "amount": 1,
///         "ignore_unbreaking": true
///     }
/// }
/// ```
/// 
/// This example will slightly damage all items with an armor value.
